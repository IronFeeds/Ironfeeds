<form action="/" method="post">

</form>
{{#each name}}
{{#if this.image}}
<img src="{{this.image}}" alt="{{this.title}}">
{{/if}}
<h2>{{this.title}}</h2>
<p>{{this.published_at}}</p>
<p>{{this.description}}</p>
<p><span>Source:{{this.source}}</span> <span>Category:{{this.category}}</span> <span>Country:{{this.country}}</span></p>
<a href="{{this.url}}">Read the article</a>
{{/each}}